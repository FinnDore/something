{
    "root": "apps/something",
    "projectType": "application",
    "sourceRoot": "apps/something/src",
    "targets": {
        "build": {
            "executor": "@nrwl/workspace:run-commands",
            "command": "cargo build --workspace --target-dir ./dist/apps/something/",
            "configurations": {
                "production": {
                    "command": "cargo build --workspace --target-dir ./dist/apps/something/ --release"
                },
                "development": {
                    "command": "cargo build --workspace --target-dir ./dist/apps/something/"
                }
            }
        },
        "serve": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "commands": ["cd ./apps/something", "cargo watch -x run"]
            },
            "configurations": {
                "production": {
                    "commands": [
                        "cd ./apps/something",
                        "cargo watch -x \"run --release\""
                    ]
                },
                "development": {
                    "options": {
                        "commands": [
                            "cd ./apps/something",
                            "cargo watch -x run"
                        ]
                    }
                }
            }
        },
        "test": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "commands": ["cd ./apps/something", "cargo test"]
            },
            "configurations": {
                "watch": {
                    "commands": ["cd ./apps/something", "cargo watch -x test"]
                }
            }
        },
        "lint": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "commands": ["cd ./apps/something", "cargo check --release"]
            }
        }
    },
    "tags": []
}
