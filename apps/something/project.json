{
    "root": "apps/something",
    "projectType": "application",
    "sourceRoot": "apps/something/src",
    "targets": {
        "build": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "outputPath": "dist/apps/something/debug/something.exe",
                "command": "cargo build --workspace --target-dir ./dist/apps/something/"
            },
            "configurations": {
                "production": {
                    "outputPath": "dist/apps/something/releases/something.exe",
                    "command": "cargo build --workspace --target-dir ./dist/apps/something/ --release"
                },
                "development": {
                    "outputPath": "dist/apps/something/debug/something.exe",
                    "command": "cargo build --workspace --target-dir ./dist/apps/something/"
                }
            }
        },
        "serve": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "commands": ["cd ./apps/something", "cargo watch -x run"]
            },
            "configurations": {
                "production": {
                    "commands": [
                        "cd ./apps/something",
                        "cargo watch -x \"run --release\""
                    ]
                },
                "development": {
                    "commands": ["cd ./apps/something", "cargo watch -x run"]
                }
            }
        },
        "test": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "commands": ["cd ./apps/something", "cargo test"]
            },
            "configurations": {
                "watch": {
                    "commands": ["cd ./apps/something", "cargo watch -x test"]
                }
            }
        },
        "lint": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "commands": ["cd ./apps/something", "cargo check --release"]
            }
        },
        "version": {
            "executor": "@jscutlery/semver:version",
            "options": {
                "preset": "conventional",
                "commitMessageFormat": "chore(${projectName}): release version ${version}",
                "postTargets": ["something:rust-version"]
            }
        },
        "rust-version": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "command": "cargo set-version ${version}"
            }
        }
    },
    "tags": []
}
